<div class="profile-header" style="background-image: url(../assets/img/iceland.jpg);">
  <div class="container">
    <div class="container-inner">
      <div class="rounded-circle media-object" style="width: 150px; height: 150px; background-size: cover; background-position: top center; background-image: url(<%= profile.img %>)"></div>
      <h3 class="profile-header-user"><%= profile.firstName + ' ' + profile.lastName %></h3>
      <p class="profile-header-bio">
        <% if (profile.bio) { %>
          <%= profile.bio %>
        <% } else if (editor) { %>
          Say something about yourself
        <% } %>
      </p>
    </div>
  </div>

  <nav class="profile-header-nav">
    <ul class="nav nav-tabs justify-content-center">
      <li class="nav-item active">
        <a class="nav-link collection" id="feed" href="#">Feed</a>
      </li>
      <li class="nav-item">
        <a class="nav-link collection" id="projects" href="#">Projects</a>
      </li>
      <li class="nav-item">
        <a class="nav-link collection" id="photos" href="#">Photos</a>
      </li>
    </ul>
  </nav>
</div>

<div id='profileBody' class='container pt-4'>
  <div class='row'>
    <div class='col-md-3'>
      <div class="card about visible-md-block visible-lg-block mb-4">
        <div class="card-block">
          <h6 class="mb-3">About <small>· <a href="#">Edit</a></small></h6>
          <ul class="list-unstyled list-spaced">
            <li><span class="text-muted icon icon-calendar mr-3"></span>Went to <a href="#">Oh, Canada</a>
            <li><span class="text-muted icon icon-users mr-3"></span>Became friends with <a href="#">Obama</a>
            <li><span class="text-muted icon icon-github mr-3"></span>Worked at <a href="#">Github</a>
            <li><span class="text-muted icon icon-home mr-3"></span>Lives in <a href="#">San Francisco, CA</a>
            <li><span class="text-muted icon icon-location-pin mr-3"></span>From <a href="#">Seattle, WA</a>
          </ul>
        </div>
      </div>
    </div>
    <div class='col-md-9'>
      <div>
        <ul class="list-group media-list media-list-stream mb-4">
          <li id='postCard' class="card media list-group-item p-4">
            <form id='newPost' method='post'>
              <% var postPlaceholder = '' %>
              <% if (user._id.equals(profile._id)) { %>
                <% postPlaceholder = 'What are you working on?' %>
              <% } else { %>
                <% postPlaceholder = 'Say something to ' + profile.firstName %>
              <% } %>
              <textarea id='postInput' method='post' form='newPost' type='text' name='postInput' placeholder='<%= postPlaceholder %>'></textarea>
              <div id='postButtons'>
                <button class='btn btn-md btn-secondary'><span class='icon icon-camera'></span></button>
                <button class='btn btn-md btn-primary'><span class='icon icon-paper-plane'></span></button>
              </div>
            </form>
          </li>
            <div id='posts'>
            <% profile.posts.forEach((post) => { %>
              <li id="<%= post._id %>" class="card media list-group-item p-4 post">
              <img
                class="media-object d-flex align-self-start mr-3"
                src="<%= post.author.img %>">
              <div class="media-body">
                <div class="media-body-text">
                  <div class="media-heading">
                    <small class="float-right text-muted"><span class='timeStamp'><%= post.date %></span></small>
                    <h6>
                      <a href='./<%= post.author.id %>'><%= post.author.name %></a>
                      <% if (!post.author.id.equals(post.user.id)) { %>
                        <span class="icon icon-triangle-right"></span>
                        <a href='./<%= post.user.id %>'><%= post.user.name %></a>
                      <% } %>
                    </h6>
                  </div>
                  <p>
                    <%= post.content %>
                  </p>
                </div>
                <div class='d-flex'>
                  <div class="mr-auto pt-1">
                    <a class="likes" href="#userModal" data-toggle="modal"><span class="icon icon-heart"></span><span><%= ' ' + post.likes.length %></span> Likes</a>
                    <span>&nbsp</span>
                    <a class="comments" href=#><span class="icon icon-message"></span><span><%= ' ' + post.comments.length %></span> Comments</a>
                  </div>
                  <div>
                    <% var liked = false; %>
                    <% post.likes.forEach(function(like) { %>
                      <% if (like.id.equals(user._id)) { %>
                        <% liked = true %>
                      <% } %>
                    <% }) %>
                    <% if (!liked) { %>
                      <button class='btn btn-sm btn-outline-secondary like'><span class='icon icon-heart' style='color: red'></span></button>
                    <% } %>
                    <button class='btn btn-sm btn-outline-secondary comment'><span class='icon icon-message' style='color: #3097D1'></span></button>
                    <button class='btn btn-sm btn-outline-secondary share'><span class='icon icon-retweet' style='color: green'></span></button>
                  </div>
                </div>
                  <ul class="media-list comment-list hidden">
                   <li class="media mb-3">

                    <img
                      class="media-object d-flex align-self-start mr-3"
                      src="<%= user.img %>">
                    <div class="media-body">
                      <form class="com-form" method="post">
                        <input class="com-in mt-2" type="text" placeholder="Write a comment...">
                      </form>
                    </div>
                  </li>
                </ul>
              </div>


            </li>

            <% }) %>
          </div>
        </ul>
      </div>
</div>

